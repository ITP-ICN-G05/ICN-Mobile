{
  "expo": {
    "name": "ICN Navigator",
    "slug": "icn-navigator-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F99F1C"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.icnvictoria.navigator",
      "config": {
        "googleMapsApiKey": "AIzaSyBsJb-fpJij7dFzZC9QbO4DsGXlw1c2-n0"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "ICN Navigator uses your location to show nearby companies on the map."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F99F1C"
      },
      "package": "com.icnvictoria.navigator",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBsJb-fpJij7dFzZC9QbO4DsGXlw1c2-n0"
        }
      },
      "permissions": ["ACCESS_FINE_LOCATION"]
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-location",
        {
          "isAndroidBackgroundLocationEnabled": false,
          "locationAlwaysAndWhenInUsePermission": "Allow ICN Navigator to access your location for showing nearby companies."
        }
      ]
    ],
    "scheme": "icn-navigator"
  }
}
